    "@orpc/tanstack-query": "catalog:orpc",
    "@sentry/tanstackstart-react": "catalog:sentry",
    "@starter/api": "workspace:*",
    "@starter/auth": "workspace:*",
    "@starter/env": "workspace:*",
    "@starter/logging": "workspace:*",
    "@starter/schemas": "workspace:*",
    "@starter/ui": "workspace:*",
    "@tanstack/react-form": "catalog:tanstack",
    "@tanstack/react-query": "catalog:tanstack",
    "@tanstack/react-router": "catalog:tanstack",
    "@tanstack/react-router-ssr-query": "catalog:tanstack",
    "@tanstack/react-start": "catalog:tanstack",
    "better-auth": "catalog:auth",
    "better-themes": "catalog:ui",
    "lucide-react": "catalog:ui",
    "react": "catalog:react",
    "react-dom": "catalog:react",
