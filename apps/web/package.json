{
  "name": "@starter/web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun --bun vite build && bun build instrument.server.ts --outdir .output/server --target bun",
    "clean": "git clean -xdf .output node_modules dist .turbo .tanstack .tsbuildinfo",
    "dev": "bun --bun vite dev --port 3000",
    "preview": "bun --bun vite preview",
    "start": "bun --import ./.output/server/instrument.server.js .output/server/index.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@opentelemetry/instrumentation-http": "catalog:otel",
    "@opentelemetry/sdk-node": "catalog:otel",
    "@orpc/client": "catalog:orpc",
